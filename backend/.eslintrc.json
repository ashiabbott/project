{
  "env": {
    "browser": false,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": "warn",
    "no-console": "off",
    "no-underscore-dangle": "off"
  }
}
